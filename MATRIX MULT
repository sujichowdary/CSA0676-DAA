#include<stdio.h>
int main()
{
	int a[10][10],b[10][10],c[10][10],n,m,p,q,i,j,k;
	printf("enter no of rows in a:\n");
	scanf("%d",&n);
	printf("enter no of column in a:\n");
	scanf("%d",&m);
	printf("enter no of rows in b:\n");
	scanf("%d",&p);
	printf("enter no of column in b:\n");
	scanf("%d",&q);
	if(m==p)
	{
		printf("enter elements in array a:\n");
		for(i=0;i<n;i++)
		{
			for(j=0;j<m;j++)
			{
				scanf("%d",&a[i][j]);
			}
		}
		printf("enter elements in array b:\n");
		for(i=0;i<p;i++)
		{
			for(j=0;j<q;j++)
			{
				scanf("%d",&b[i][j]);
			}
		}
		printf("resultant array:\n");
		for(i=0;i<n;i++)
		{
			for(j=0;j<p;j++)
			{
				c[i][j]=0;
				for(k=0;k<n;k++)
				{
					c[i][j]+=a[i][k]*b[k][j];
				}
			}
		}
		for(i=0;i<n;i++)
		{
			for(j=0;j<n;j++)
			{
				printf("%d\t",c[i][j]);
			}
		}
		
	}
	
}
